{
  "name": "se-risktrac-grc-um-api",
  "version": "1.0.0",
  "description": "SE-RiskTrac user management module application.",
  "main": "app-server.js",
  "bin": "app-server.js",
  "scripts": {
    "start": "node app-server.js",
    "dev": "nodemon --inspect=9002 app-server.js",
    "build": "npx gulp --gulpfile build-scripts\\gulpfile.js build",
    "gulp-scripts": "npx gulp --gulpfile build-scripts\\gulpfile.js scripts",
    "package": "npx gulp --gulpfile build-scripts\\gulpfile.js package",
    "buildall": "npm run build && npm run gulp-scripts && npm run package"
  },
  "pkg": {
    "scripts": [
      "app/**/*.js"
    ],
    "targets": [
      "node10.16.1"
    ]
  },
  "dependencies": {
    "app-root-path": "3.1.0",
    "axios": "0.27.2",
    "bcrypt": "^5.1.1",
    "body-parser": "1.20.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.1",
    "express-fileupload": "1.4.0",
    "helmet": "6.0.0",
    "jsonwebtoken": "8.5.1",
    "jsonwebtoken-refresh": "5.7.0",
    "moment": "2.29.4",
    "mssql": "5.1.0",
    "nocache": "3.0.4",
    "node-cron": "^3.0.3",
    "node-jsencrypt": "1.0.0",
    "nodemailer": "^6.9.16",
    "path": "0.12.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "13.7.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "ansi-colors": "^4.1.3",
    "browser-sync": "^2.29.3",
    "del": "^5.1.0",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-build": "^0.5.3",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^3.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.1.0",
    "gulp-sequence": "^1.0.0",
    "gulp-terser": "^2.1.0",
    "gulp-uglify-es": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^5.1.0",
    "gulplog": "^2.2.0",
    "merge-stream": "^2.0.0",
    "nodemon": "^3.1.10",
    "plugin-error": "^2.0.1"
  },
  "author": "SecurEyes",
  "license": "ISC"
}
