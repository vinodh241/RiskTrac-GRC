{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch User-Management in Chrome (nginx /user-management/)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5000/user-management/",
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe",
      "runtimeArgs": [
        "--remote-debugging-port=9223",
        "--user-data-dir=${workspaceFolder}\\.vscode\\chrome-profile"
      ],
      "sourceMaps": true
    },
    {
      "name": "Attach to User-Management Chrome (9223)",
      "type": "chrome",
      "request": "attach",
      "port": 9223,
      "webRoot": "${workspaceFolder}"
    }
  ]
}
